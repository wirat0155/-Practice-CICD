{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "error",
        "no-console": "warn",
        "eqeqeq": [
            "error",
            "always"
        ],
        "curly": "error",
        "no-var": "error",
        "prefer-const": "error"
    }
}